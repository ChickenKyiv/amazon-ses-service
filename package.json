{
  "name": "nonprofit-email-service",
  "version": "1.0.0",
  "description": "A cost effective, open source solution to sending mass emails",
  "main": "index.js",
  "engines": {
    "npm": ">=5",
    "node": ">=7.6.0"
  },
  "scripts": {
    "check-env-file": "python ./tools/setup/initial_setup.py",
    "client:lint": "esw webpack.config.* client tools --color",
    "client:clean-dist": "npm run client:remove-dist && mkdir dist",
    "start": "NODE_ENV=production npm-run-all --parallel start:*",
    "start:main": "node server/index.js",
    "start:consumer": "node server/feedback-consumer/consumer.js",
    "production": "DEV_SEND_RATE=200 TEST_PRODUCTION=true NODE_ENV=production node server/index.js",
    "dev": "DEV_SEND_RATE=200 NODE_ENV=development node server/index.js",
    "debug": "DEV_SEND_RATE=200 NODE_ENV=development node --inspect --debug-brk server/index.js",
    "build": "babel-node tools/build.js",
    "simulator": "node ./node_modules/node-amazon-ses-simulator/index.js -e 0",
    "feedback-consumer": "NODE_ENV=production node server/feedback-consumer/index.js",
    "test:server": "npm run test:server:unit && npm run test:server:functional",
    "test:server:unit": "mocha tools/testSetup.js \"server/**/*.spec.js\"",
    "test:server:functional": "babel-tape-runner -r dotenv/config server/tests/**/*.func.js | node_modules/.bin/tap-spec",
    "test:client": "mocha --require tools/testClientSetup.js tools/testSetup.js \"client/**/*.spec.js\"",
    "test": "npm run test:client && npm run test:server",
    "updateDB": "node_modules/.bin/sequelize db:migrate --env production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FreeCodeCamp/nonprofit-email-service.git"
  },
  "author": "FreeCodeCamp",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/FreeCodeCamp/nonprofit-email-service/issues"
  },
  "homepage": "https://github.com/FreeCodeCamp/nonprofit-email-service#readme",
  "dependencies": {
    "admin-lte": "2.3.6",
    "async": "^3.2.0",
    "aws-sdk": "^2.709.0",
    "bcrypt": "^5.0.0",
